 default {\n  data() {\n    return {\n      isActive: false,\n      start: null,\n      end: null,\n      timer: 0,\n      interval: null,\n      logs: []\n    }\n  },\n  methods: {\n    handleClick() {\n      if (this.isActive) {\n        this.end = new Date().toLocaleDateString('en', { hour:\"numeric\", minute:\"numeric\"})\n        const log = `Someone worked from ${this.start} to ${this.end}.`\n        this.logs.push(log)\n      } else {\n        clearInterval(this.interval);\n      }\n      this.isActive = !this.isActive\n    }\n  },\n  computed: {\n    timestamp() {\n      if (this.isActive) {\n       var date = new Date(null);\n       date.setSeconds(this.timer);\n       return date.toISOString().substr(11, 8);\n      } else {\n        return '';\n      }\n    }\n  },\n  watch: {\n    isActive: {\n      immediate: true,\n      handler(boolean) {\n        if (boolean) {\n          this.start = new Date().toLocaleDateString('en', { hour:\"numeric\", minute:\"numeric\"})\n          let _ = this\n          this.interval = setInterval(function () {\n            _.timer ++;\n          }, 1000);\n        }\n      }\n    }\n  },\n  mounted() {\n    const myObserver = new ResizeObserver(entries => {\n      entries.forEach(entry => {\n        const element = this.$refs.logs;\n        element.scrollTo({\n         top: element.scrollHeight,\n         behavior: 'smooth',\n       });\n      });\n    });\n\n    myObserver.observe(this.$refs.logsContainer);\n    setTimeout(()=>{\n      this.$router.push('/3')\n    }, 10000)\n  }\n}\n\n</script>\n"]}]}