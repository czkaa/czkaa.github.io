  }\n        ]\"\n      />\n    </div>\n\n\n    <div snippet-logs\n      ref=\"logs\"\n      :class=\"[\n        'fixed top-0 left-0',\n        'w-full h-screen',\n        'p-2 pt-12',\n        'overflow-y-scroll'\n      ]\"\n    >\n      <div ref=\"logsContainer\"\n      :class=\"[\n        'flex flex-wrap',\n        'justify-center'\n      ]\">\n        <template v-for=\"log in logs\">\n          <div v-for=\"index in log.duration\"\n            :style=\"{\n              width: log.person === '0' ? '4vw' : '20vw',\n              background: `rgba(0,0,0,${log.person})`\n            }\"\n            :class=\"[\n              'h-balken',\n              log.person === '0' ? 'isEmpty' : 'isWork'\n            ]\"\n          >\n          </div>\n        </template>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n\nexport default {\n  data() {\n    return {\n      isActive: false,\n      timer: 0,\n      interval: null,\n      start: null,\n      end: null,\n      person: null,\n      logs: [\n        {person: 0.3, duration: 2},\n        {person: 0.6, duration: 7},\n        {person: 0.0, duration: 4},\n        {person: 0.0, duration: 7},\n        {person: 0.5, duration: 4},\n        {person: 0.9, duration: 2},\n        {person: 0.0, duration: 1},\n        {person: 0.6, duration: 8},\n        {person: 0.3, duration: 10},\n        {person: 0.0, duration: 20},\n        {person: 0.5, duration: 2},\n        {person: 0.0, duration: 4},\n        {person: 0.3, duration: 2},\n        {person: 0.9, duration: 3},\n        {person: 0.0, duration: 4},\n        {person: 0.3, duration: 10},\n        {person: 0.6, duration: 2},\n        {person: 0.3, duration: 1},\n        {person: 0.5, duration: 8},\n        {person: 0.9, duration: 10},\n        {person: 0.0, duration: 7},\n        {person: 0.5, duration: 4},\n        {person: 0.6, duration: 7},\n        {person: 0.0, duration: 4},\n      ]\n    }\n  },\n  methods: {\n    handleClick() {\n      this.isActive = !this.isActive\n      setTimeout(()=> {\n\n      }, 100);\n    }\n  },\n  computed: {\n    timestamp() {\n      if (this.isActive) {\n        var date = new Date(null);\n       date.setSeconds(this.timer);\n       return date.toISOString().substr(11, 8);\n      } else {\n        return '00:00:00';\n      }\n    }\n  },\n  watch: {\n    isActive: {\n      immediate: false,\n      handler(boolean) {\n        if (!boolean) {\n          this.person = [\"0.3\", \"0.6\", \"0.9\", \"0.5\"][ Math.floor(Math.random() * 3)];\n          const log = {person: this.person, duration: this.timer}\n          this.logs.push(log)\n        } else {\n          this.person = \"0\"\n          const log = {person: this.person, duration: this.timer}\n          this.logs.push(log)\n        }\n        this.timer = 0;\n      }\n    }\n  },\n  mounted() {\n    const myObserver = new ResizeObserver(entries => {\n      entries.forEach(entry => {\n        const element = this.$refs.logs;\n        console.log('resize')\n        element.scrollTo({\n         top: element.scrollHeight,\n         behavior: 'smooth',\n       });\n      });\n    });\n\n    let _ = this\n    this.interval = setInterval(function () {\n      _.timer ++;\n    }, 1000);\n\n    myObserver.observe(this.$refs.logsContainer);\n  }\n}\n\n</script>\n"]}]}