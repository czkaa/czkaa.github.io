,\n  components: {\n  },\n  methods: {\n    hoverItem(array) {\n      if (array !== []) {\n        this.hasEvents = false\n        this.selectedItems = [];\n        setTimeout(() => {\n          this.selectedItems = array;\n        }, 1000)\n        setTimeout(() => {\n          this.hasEvents = true\n        }, 2000)\n      } else {\n        this.selectedItems = [];\n      }\n    },\n    // hoverItem(i) {\n    //   this.hoveredItem = null;\n    //   setTimeout(() => {\n    //     this.hoveredItem = i;\n    //   }, 1000)\n    // }\n  },\n  mounted() {\n    axios.get('research.json')\n   .then(res => this.research = res.data)\n   .catch(err => console.log(err))\n  }\n}\n</script>\n\n\n<style lang=\"scss\">\n\n\n.shrink-enter-active {\n  @apply animate-shrink;\n  animation-duration: 800ms;\n}\n.shrink-leave-active {\n  @apply animate-shrink;\n  animation-direction: reverse;\n  animation-duration: 800ms;\n}\n\n</style>\n"]}]}