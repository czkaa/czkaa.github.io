ACN,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACf;YACE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACtB;QACF,CAAC,OAAO,CAAC;;IAEb,CAAC,CAAC,CAAC,CAAC,CAAC;;EAEP,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/karen/Desktop/sose22 – candy desk/website__zeiterfassung/src/components/page/Page4.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"w-full h-full\"\n  :class=\"[\n    'w-full h-full',\n    'flex justify-end',\n    'p-2 gap-2'\n  ]\">\n  {{counter}}\n    <div snippet-button\n      class=\"w-full\"\n    >\n      <BasicButton\n        :isActive=\"isOnline\"\n        :class=\"[\n          'w-full h-full',\n          {\n            'bg-black' : isOnline\n          }\n        ]\"      />\n\n    </div>\n\n  </div>\n</template>\n\n<script>\n\nimport $ from 'jquery'\nimport axios from 'axios'\n\n\nexport default {\n  data() {\n    return {\n      timer: \"\",\n      interval: null,\n      logs: null,\n      counter: 0,\n      isActive: false,\n      isBlocked: false,\n      num: 3,\n    }\n  },\n  computed: {\n    isOnline() {\n      return (this.counter === this.num)\n    }\n  },\n  watch: {\n    isOnline: {\n      immediate: false,\n      handler(boolean) {\n        if (boolean) {\n          const favicon = document.querySelectorAll(\"link\")\n          console.log(favicon)\n          // favicon.href = \"https://www.google.com/favicon.ico\";\n        }\n      },\n    },\n  },\n  created() {\n    document.addEventListener('beforeunload', this.handler)\n  },\n  methods: {\n    handler: function handler(event) {\n      this.decrementCounter();\n      localStorage.isActive = false;\n    },\n    incrementCounter() {\n      $.ajax({\n          contentType: \"application/json\", // php://input\n          dataType : \"json\",\n          method: \"POST\",\n          processData: false,\n          // url: \"http://localhost:8888/incrementCounter.php\",\n          url: \"//thumb.kew-halle.de/incrementCounter.php\",\n          data: JSON.stringify(1)\n      })\n      .done(function(data) {\n          console.log(data);\n      })\n      .fail(function(data) {\n          console.log(data);\n      });\n    },\n    decrementCounter() {\n      $.ajax({\n          contentType: \"application/json\", // php://input\n          dataType : \"json\",\n          method: \"POST\",\n          processData: false,\n          // url: \"http://localhost:8888/decrementCounter.php\",\n          url: \"//thumb.kew-halle.de/decrementCounter.php\",\n          data: JSON.stringify(1)\n      })\n      .done(function(data) {\n          console.log(data);\n      })\n      .fail(function(data) {\n          console.log(data);\n      });\n    },\n    getContent() {\n      axios.get('counter.json')\n     .then(res => this.counter = res.data)\n     .catch(err => console.log(err))\n     console.log(this.counter)\n    }\n  },\n  mounted() {\n    this.incrementCounter();\n    let _ = this\n    this.interval = setInterval(function () {\n      if (!this.isBlocked) {\n        _.getContent()\n      }\n    }, 1000);\n    // this.getContent();\n    // this.incrementCounter();\n    //\n    // if (localStorage.isActive === undefined) {\n    //   localStorage.isActive = false\n    //   this.isActive = false;\n    // } else if (JSON.parse(localStorage.isActive) === true) {\n    //   this.isActive = true;\n    // } else {\n    //   this.isActive = false;\n    // }\n  },\n  beforeUnmount(){\n    clearInterval(this.interval);\n  }\n}\n\n</script>\n"]}]}