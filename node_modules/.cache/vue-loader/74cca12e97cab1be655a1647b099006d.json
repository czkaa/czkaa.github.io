eiterfassung/src/components/page/PageHome.vue","sourceRoot":"","sourcesContent":["<template>\n\n  <div :style=\"{fontSize: '2.5vw'}\" class=\"text-sm absolute left-0 leading-none -top-0.5\">working</div>\n  <div :style=\"{fontSize: '2.5vw'}\" class=\"text-sm absolute left-1/3 ml-0.5 -top-0.5 leading-none\">time</div>\n  <div :style=\"{fontSize: '2.5vw'}\" class=\"text-sm absolute left-2/3 ml-0.5 -top-0.5 leading-none\">recordings</div>\n  <div page-home class=\"w-full h-full\"\n  :class=\"[\n    'flex justify-center items-center'\n  ]\">\n    <!-- <div\n      :style=\"{fontSize: '1rem', borderRadius: '100%', fontFamily: 'Apple Chancery', paddingBotton: '100%'}\"\n      :class=\"[\n      ]\"\n    >\n      <div class=\"cursor-pointer animate-rotate\">working hours (recording)</div>\n    </div> -->\n\n    <!-- <div\n      :class=\"[\n        'absolute left-40',\n        'h-full',\n        'border-l border-black'\n      ]\"\n    />\n    <div\n      :class=\"[\n        'absolute left-20',\n        'h-full',\n        'border-l border-black'\n      ]\"\n    />\n    <div\n      :class=\"[\n        'absolute right-60',\n        'h-full',\n        'border-l border-black'\n      ]\"\n    /> -->\n    <div class=\"w-3/4\">\n      <button\n        @click=\"handleClick(1)\"\n        :style=\"{fontSize: '8vw', borderRadius: '0.25rem', fontFamily: 'sans-serif'}\"\n        :class=\"[\n          'w-full',\n          'flex flex-col items-end',\n          'transition-all duration-stempel',\n          'leading-none',\n          'uppercase',\n          selected === null || selected === 1 ? 'opacity-1' : 'opacity-0 pointer-events-none'\n        ]\">\n          <div class=\"lowercase\" :style=\"{fontSize: '1rem', fontFamily: 'Apple-Chancery'}\">(part 1) </div>\n          <div>synchronity</div>\n      </button>\n\n      <button\n        @click=\"handleClick(2)\"\n        :style=\"{fontSize: '8vw', borderRadius: '0.25rem', fontFamily: 'sans-serif'}\"\n        :class=\"[\n          'w-full',\n          'flex flex-col items-start',\n          'transition-all duration-stempel',\n          'leading-none',\n          'uppercase',\n          selected === null || selected === 2 ? 'opacity-1' : 'opacity-0 pointer-events-none'\n        ]\">\n          <div class=\"lowercase\" :style=\"{fontSize: '1rem', fontFamily: 'Apple-Chancery'}\">(part 2) </div>\n          <div>rythm</div>\n      </button>\n\n      <button\n        @click=\"handleClick(3)\"\n        :style=\"{fontSize: '8vw', borderRadius: '0.25rem', fontFamily: 'sans-serif'}\"\n        :class=\"[\n          'w-full',\n          'flex flex-col items-center',\n          'transition-all duration-stempel',\n          'leading-none',\n          'uppercase',\n          selected === null || selected === 3 ? 'opacity-1' : 'opacity-0 pointer-events-none'\n        ]\">\n          <div class=\"lowercase\" :style=\"{fontSize: '1rem', fontFamily: 'Apple-Chancery'}\">(part 3) </div>\n          <div>separability</div>\n      </button>\n\n    </div>\n\n    <SnippetTimetable :selected=\"selected\" :groupSize=\"groupSize\"/>\n\n    <SnippetGroupSize  @increment=\"groupSize ++\" @decrement=\"groupSize --\" v-if=\"selected === null\" class=\"absolute bottom-0 right-0\" :groupSize=\"groupSize\" />\n\n\n    <SnippetSketch1 v-if=\"selected === 1\" :status=\"status\" :groupSize=\"groupSize\"/>\n    <SnippetSketch2 v-if=\"selected === 2\" :logs=\"logs\" :groupSize=\"groupSize\"/>\n    <SnippetSketch3 v-if=\"selected === 3\" :time=\"time\" :groupSize=\"groupSize\"/>\n\n    <SnippetQuestions class=\"z-50\" v-if=\"selected !== null\" :questions=\"questions[selected-1]\" :clicks=\"clicks\"/>\n\n\n  </div>\n\n\n</template>\n\n<script>\n\nexport default {\n  name: 'PageHome',\n  components: {\n  },\n  data() {\n    return {\n      counter: 0,\n      selected: null,\n      status: null,\n      logs: null,\n      clicks: 0,\n      groupSize: 3,\n      time: null,\n      questions: [\n        ['What changes when we work at the same time?', 'Do we take breaks together?', 'What changes when one person is missing?', 'Do we begin and end at the same time?'],\n        ['How much of our time do we devote to he group work?', 'Do we want to work according to a fixed schedule?', 'How spontaneous can we be as group?'],\n        ['Do we want to record individual working hours at all?', 'Whast does this mean in terms of compensation?', 'Is there a necessary level of control?']\n      ]\n    }\n  },\n  watch: {\n    status(oldVal, newVal) {\n      console.log(newVal)\n      if (oldVal && newVal) {\n        for (let j = 0; j < this.groupSize; j++) {\n          if (oldVal[j] !== newVal[j]) {\n            this.clicks ++;\n            return;\n          }\n        }\n      }\n    }\n  },\n  methods: {\n    openTab(top, left, index) {\n      let route = this.$router.resolve({ name: 'Button', query: {person: index}  });\n      window.open(\n        route.href,\n        'targetWindow' + index,\n        `toolbar=no,location=no,status=no,menubar=no,scrollbars=no,resizable=yes,width=${200},height=${200},  top=${top}, left=${left}`\n      );\n    },\n    handleClick(i) {\n      if (i !== this.selected) {\n        this.selected = i;\n\n        for (let j = 0; j < this.groupSize; j++) {\n          this.openTab(Math.floor(Math.random() * 600) + 300, Math.floor(Math.random() * 600) + 300, j)\n          console.log('open')\n        }\n      } else {\n        this.selected = null\n        this.initStorage();\n      }\n      this.clicks = 0;\n    },\n    updateStorage() {\n      this.status = JSON.parse(localStorage.getItem('status'));\n      this.logs = JSON.parse(localStorage.getItem('logs'));\n      this.time = JSON.parse(localStorage.getItem('time'));\n    },\n    initStorage() {\n      let status = {};\n      let logs = {};\n      let time = {}\n      for (let i = 0; i < this.groupSize; i++) {\n        status[i] = false;\n        time[i] = 0;\n      }\n      localStorage.setItem('status', JSON.stringify(status))\n      localStorage.setItem('logs', JSON.stringify(logs))\n      localStorage.setItem('time', JSON.stringify(time))\n    }\n  },\n  mounted() {\n    this.initStorage();\n\n    window.addEventListener('storage', () => {\n      this.updateStorage();\n    })\n  },\n}\n</script>\n\n\n<style lang=\"scss\">\n\n  .animate-rotate {\n    &:hover {\n      animation-play-state: paused;\n    }\n  }\n\n</style>\n"]}]}