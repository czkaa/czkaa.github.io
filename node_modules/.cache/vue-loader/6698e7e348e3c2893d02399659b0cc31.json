dler(boolean) {\n        if (!boolean) {\n          clearInterval(this.interval);\n          this.timer = \"\";\n          const person = [\"A\", \"B\", \"C\"][ Math.floor(Math.random() * 3)];\n          const timestamp = new Date().toLocaleDateString('en', { year:\"numeric\", month:\"numeric\", day:\"numeric\", hour:\"numeric\", minute:\"numeric\"})\n          const log = `${timestamp}: ${person} stopped working`\n          this.logs.push(log)\n        } else {\n          this.timer = 0;\n          let _ = this\n          this.interval = setInterval(function () {\n            _.timer ++;\n          }, 1000);\n        }\n      }\n    }\n  },\n  mounted() {\n    const myObserver = new ResizeObserver(entries => {\n      entries.forEach(entry => {\n        const element = this.$refs.logs;\n        element.scrollTo({\n         top: element.scrollHeight,\n         behavior: 'smooth',\n       });\n      });\n    });\n\n    myObserver.observe(this.$refs.logsContainer);\n  }\n}\n\n</script>\n"]}]}