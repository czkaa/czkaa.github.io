AAC,CAAC,CAAC,CAAC;QACnB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACnB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACN,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACf;YACE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACtB;QACF,CAAC,OAAO,CAAC;;IAEb,CAAC,CAAC,CAAC,CAAC,CAAC;;EAEP,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/karen/Desktop/sose22 – candy desk/website__zeiterfassung/src/components/page/Page4.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"w-full h-full\"\n  :class=\"[\n    'w-full h-full',\n    'flex justify-end',\n    'p-2 gap-2'\n  ]\">\n  {{isOnline}} / {{counter}}\n    <div snippet-button\n      class=\"w-full\"\n    >\n      <BasicButton\n        :isActive=\"isActive\"\n        @click=\"handleClick\"\n        :class=\"[\n          'w-full h-full',\n          {\n            'bg-black' : isActive\n          }\n        ]\"      />\n\n    </div>\n\n  </div>\n</template>\n\n<script>\n\nimport $ from 'jquery'\nimport axios from 'axios'\n\n\nexport default {\n  data() {\n    return {\n      timer: \"\",\n      interval: null,\n      logs: null,\n      counter: 0,\n      isActive: false,\n      isBlocked: false,\n      num: 3,\n    }\n  },\n  computed: {\n    isOnline() {\n      return (this.counter === this.num)\n    }\n  },\n  watch: {\n    isOnline: {\n      immediate: false,\n      handler(boolean) {\n        if (boolean) {\n          const favicon = document.querySelector(\"#favicon\")\n          console.log(favicon)\n          // favicon.href = \"https://www.google.com/favicon.ico\";\n        }\n      },\n    },\n  },\n  methods: {\n    handleClick() {\n      if (this.isActive) {\n        localStorage.isActive = false;\n        this.isActive = false;\n        this.postCounter(-1);\n      } else {\n        localStorage.isActive = true;\n        this.isActive = true;\n        this.postCounter(1);\n      }\n    },\n    postCounter(val) {\n      $.ajax({\n          contentType: \"application/json\", // php://input\n          dataType : \"json\",\n          method: \"POST\",\n          processData: false,\n            url: \"http://localhost:8888/counter.php\",\n          // url: \"//fingerprint.identification-ceremonies.watch/postFingerprint.php\",\n          data: JSON.stringify(this.counter + val)\n      })\n      .done(function(data) {\n          console.log(data);\n      })\n      .fail(function(data) {\n          console.log(data);\n      });\n    },\n    getContent() {\n      axios.get('counter.json')\n     .then(res => console.log(this.counter = res.data))\n     .catch(err => console.log(err))\n     console.log(this.counter)\n    }\n  },\n  mounted() {\n    let _ = this\n    this.interval = setInterval(function () {\n      if (!this.isBlocked) {\n        _.getContent()\n      }\n    }, 1000);\n\n    if (localStorage.isActive === undefined) {\n      localStorage.isActive = false\n      this.isActive = false;\n    } else if (JSON.parse(localStorage.isActive) === true) {\n      this.isActive = true;\n    } else {\n      this.isActive = false;\n    }\n  },\n  beforeUnmount(){\n    clearInterval(this.interval);\n  }\n}\n\n</script>\n"]}]}