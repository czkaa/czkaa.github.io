     :class=\"[\n            'pr-2',\n            'tabular-nums',\n            'transition-all duration-800',\n            'min-w-start h-40',\n            'overflow-hidden',\n            'text-center',\n            isActive ? 'max-w-end' : 'max-w-start',\n            {\n              'text-white' : isActive\n            }\n          ]\"\n        >\n          {{timestamp}}\n        </div>\n      </BasicButton>\n    </div>\n\n\n    <div snippet-logs\n      ref=\"logs\"\n      :style=\"{fontSize: '1.5rem', lineHeight: '1.05em'}\"\n      :class=\"[\n        'absolute top-0 left-0',\n        'w-full h-full',\n        'pointer-events-none',\n        'overflow-hidden',\n        'p-2'\n      ]\"\n    >\n      <div ref=\"logsContainer\">\n        <div v-for=\"log in logs\" class=\"w-full break-words\">\n          {{log}}\n        </div>\n      </div>\n    </div>\n\n\n  </div>\n</template>\n\n<script>\n\nexport default {\n  data() {\n    return {\n      isActive: false,\n      start: null,\n      end: null,\n      timer: 0,\n      interval: null,\n      logs: []\n    }\n  },\n  methods: {\n    handleClick() {\n      if (this.isActive) {\n        this.end = new Date().toLocaleDateString('en', { hour:\"numeric\", minute:\"numeric\"})\n        const log = `Someone worked from ${this.start} to ${this.end}.`\n        this.logs.push(log)\n      } else {\n        clearInterval(this.interval);\n      }\n      this.isActive = !this.isActive\n    }\n  },\n  computed: {\n    timestamp() {\n      if (this.isActive) {\n       var date = new Date(null);\n       date.setSeconds(this.timer);\n       return date.toISOString().substr(11, 8);\n      } else {\n        return '';\n      }\n    }\n  },\n  watch: {\n    isActive: {\n      immediate: true,\n      handler(boolean) {\n        if (boolean) {\n          this.start = new Date().toLocaleDateString('en', { hour:\"numeric\", minute:\"numeric\"})\n          let _ = this\n          this.interval = setInterval(function () {\n            _.timer ++;\n          }, 1000);\n        }\n      }\n    }\n  },\n  mounted() {\n    const myObserver = new ResizeObserver(entries => {\n      entries.forEach(entry => {\n        const element = this.$refs.logs;\n        element.scrollTo({\n         top: element.scrollHeight,\n         behavior: 'smooth',\n       });\n      });\n    });\n\n    myObserver.observe(this.$refs.logsContainer);\n    setTimeout(()=>{\n      this.$router.push('/3')\n    }, 10000)\n  }\n}\n\n</script>\n"],"sourceRoot":""}]}