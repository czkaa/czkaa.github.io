A,KAAK,EAAC;;;AACpB,eAAA,E;AAAU,EAAA,KAAK,EAAC;;;;;;;;SADZ,QAAA,CAAA,Q,kBAAX,mBAAA,CA6BM,KA7BN,EAAA,UAAA,EA6BM,CA5BJ,mBAAA,CAIM,KAJN,EAAA,UAAA,EAIM,E,kBAHJ,mBAAA,CAEW,SAFX,EAEW,IAFX,EAEW,WAAA,CAFiB,QAAA,CAAA,QAEjB,EAFM,OAAmB,IAAZ;;WAAoB,OAAO,CAAC;QAC3B,OAAO,CAAC,KAAR,CAAc,QAAd,OAA6B,KAAA,CAAA,KAAA,CAAM,QAAN,E,kBAApD,YAAA,CAAyG,yBAAzG,EAAyG;YAAA;AAAlC,MAAA,OAAO,EAAE,OAAyB;AAAhB,MAAA,KAAK,EAAC;AAAU,KAAzG,E,IAAA,E;;AAAA,M,WAAA,C;;;GACS,CAFX,E;;AAAA,GAGI,EAJN,CA4BI,EAvBR,mBAAA,CAAA,8QAAA,CAuBQ,EAhBJ,mBAAA,CAeM,KAfN,EAeM;AAfD,IAAA,KAAK,EAAC,iFAeL;AAfwF,IAAA,KAAK,EAAA,eAAA,CAAE;AAAA,MAAA,KAAA,EAAA;AAAA,KAAF;AAe7F,GAfN,E,iBACE,mBAAA,CAAoF,OAApF,EAAoF;AAA7E,IAAA,IAAI,EAAC,OAAwE;AAA/D,IAAA,KAAK,EAAC,iBAAyD;AAAvC,IAAA,GAAG,EAAC,MAAmC;+DAAnB,KAAA,CAAA,K,GAAK,M,CAAc;AAAZ,IAAA,GAAG,EAAC;AAAQ,GAApF,E,IAAA,E;;AAAA,G,iBAAiE,KAAA,CAAA,K,KAEjE,U,CAHF,E;;AAAA,GAgBI,CA7BN,C","sourcesContent":["<template>\n\n  <div v-if=\"channels\" class=\"flex flex-col absolute w-full h-full\">\n    <div page-home class=\"w-full h-full\">\n      <template v-for=\"channel in channels\" :key=\"channel.id\">\n        <SnippetChannel  v-if=\"channel.title.toString() === value.toString()\" :channel=\"channel\" class=\"w-full\"/>\n      </template>\n    </div>\n<!--\n    <div class=\"fixed bottom-0 right-0 bg-white flex space-x-2 pb-1 px-2 text-xs\">\n      <BasicText :text=\"`last updated ${data.updated_at} `\" />\n      <BasicLink text=\"on are.na\" :href=\"`https://www.are.na/${data.owner.slug}/${data.slug}`\" />\n    </div> -->\n\n\n    <nav class=\"fixed -top-0 left-1/2 ml-5 w-full pr-8 pl-3 transform origin-top-left rotate-90\" :style=\"{width: 'calc(100vh)'}\">\n      <input type=\"range\"  class=\"w-full h-1 z-50\" min=\"2013\" v-model=\"value\" max=\"2022\" >\n\n      <div id=\"datalist\" class=\"text-xs transform translate-y-5\">\n        <div class=\"invisible\">none</div>\n        <option value=\"2014\">2014</option>\n        <div class=\"invisible\">none</div>\n        <div class=\"invisible\">none</div>\n        <option value=\"2017\">2017</option>\n        <option value=\"2018\">2018</option>\n        <option value=\"2019\">2019</option>\n        <option value=\"2020\">2020</option>\n        <option value=\"2021\">2021</option>\n        <option value=\"2022\">2022</option>\n      </div>\n    </nav>\n  </div>\n</template>\n\n<script>\n\nimport axios from 'axios'\n\nexport default {\n  data() {\n    return {\n      root: 'my-online-portfolio',\n      data: 0,\n      value: 2013\n    }\n  },\n  computed: {\n    channels() {\n      if (this.data) {\n        return this.data.contents.filter(item => item.base_class === 'Channel');\n      } else {\n        return null\n      }\n    },\n    range() {\n      if (this.channels.length) {\n        return [parseInt(this.channels[0].title), parseInt(this.channels[this.channels.length - 1].title), parseInt(this.channels[this.channels.length - 1].title) - parseInt(this.channels[0].title) + 1]\n      } else {\n        return null\n      }\n    }\n  },\n  methods: {\n    fetchData(channelString) {\n      this.error = null\n      axios\n        .get(`https://api.are.na/v2/channels/${channelString}`)\n        .then((response) => {\n          if (response) {\n            this.data = response.data\n            console.log(this.data)\n          } else {\n            console.log('error')\n          }\n      })\n    },\n  },\n  mounted() {\n    console.log('MOUNTED')\n    this.fetchData(this.root)\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n  #datalist {\n    position:relative;\n  \tdisplay: flex;\n  \tjustify-content: space-between;\n    @apply pointer-events-none;\n    option {\n      @apply -mt-0.5 transform rotate-180 -translate-y-6 p-0.5;\n    }\n\n  }\n</style>\n"],"sourceRoot":""}]}