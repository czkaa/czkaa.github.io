sContainer\">\n        <div v-for=\"log in logs\" class=\"w-full break-words\">\n          {{log}}\n        </div>\n      </div>\n    </div>\n\n\n  </div>\n</template>\n\n<script>\n\nexport default {\n  data() {\n    return {\n      isActive: true,\n      timer: \"\",\n      interval: null,\n      logs: []\n    }\n  },\n  methods: {\n    handleClick() {\n      this.isActive = !this.isActive\n      setTimeout(()=> {\n\n      }, 100);\n    }\n  },\n  computed: {\n    timestamp() {\n      if (this.isActive) {\n        var date = new Date(null);\n       date.setSeconds(this.timer);\n       return date.toISOString().substr(11, 8);\n      } else {\n        return '';\n      }\n    }\n  },\n  watch: {\n    isActive: {\n      immediate: true,\n      handler(boolean) {\n        if (!boolean) {\n          clearInterval(this.interval);\n          this.timer = \"\";\n          const person = [\"A\", \"B\", \"C\"][ Math.floor(Math.random() * 3)];\n          const timestamp = new Date().toLocaleDateString('en', { year:\"numeric\", month:\"numeric\", day:\"numeric\", hour:\"numeric\", minute:\"numeric\"})\n          const log = `${timestamp}: ${person} stopped working`\n          this.logs.push(log)\n        } else {\n          this.timer = 0;\n          let _ = this\n          this.interval = setInterval(function () {\n            _.timer ++;\n          }, 1000);\n        }\n      }\n    }\n  },\n  mounted() {\n    const myObserver = new ResizeObserver(entries => {\n      entries.forEach(entry => {\n        const element = this.$refs.logs;\n        element.scrollTo({\n         top: element.scrollHeight,\n         behavior: 'smooth',\n       });\n      });\n    });\n\n    myObserver.observe(this.$refs.logsContainer);\n  }\n}\n\n</script>\n"],"sourceRoot":""}]}