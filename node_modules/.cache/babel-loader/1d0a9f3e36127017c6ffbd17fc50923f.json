sourcesContent":["<template>\n  <div page-button\n  :class=\"[\n    'p-2',\n    'w-full h-full'\n    /* 'flex space-x-2' */\n  ]\">\n    <button\n      v-if=\"!isActive\"\n      @click=\"handleClick\"\n      :style=\"{borderRadius: '100%', fontSize: '0.4rem',filter: 'drop-shadow(3px 2px 6px rgba(0,0,0,0.3))'}\"\n      :class=\"[\n        'h-full w-full',\n        'shadow-form',\n        'border-gray-200 border',\n        'flex-shrink-0',\n        'p-6 '\n      ]\"\n    >\n      <div\n        :style=\"{maskImage: `url(/play.svg)`, maskRepeat: 'no-repeat', maskSize: 'cover', }\"\n        :class=\"[\n          'w-full h-full',\n          'bg-white',\n          'ml-2'\n        ]\"\n      />\n    </button>\n    <button\n      v-else\n      @click=\"handleClick\"\n      :style=\"{borderRadius: '100%', fontSize: '1.5re0m', letterSpacing: '-0.01em', fontFamily: 'Apple-Chancery'}\"\n      :class=\"[\n        'h-full w-full',\n        'shadow-form',\n        'border-gray-200 border',\n        'flex-shrink-0',\n        'tabular-nums',\n        {\n          'animate-blink' : isActive\n        }\n      ]\"\n    > {{timestamp}}\n    </button>\n\n    <!-- <form\n      :style=\"{fontSize: '2rem', lineHeight: '0.95em', letterSpacing: '-0.01em'}\"\n      class=\"h-10\"\n    >\n      <input\n        :placeholder=\"person\" class=\"mb-1 w-full h-full\" type=\"text\" id=\"fname\" name=\"fname\"\n        :class=\"[\n          /* 'border border-gray-500', */\n          'rounded-lg',\n          'shadow-form',\n          'text-black',\n          'focus:outline-none',\n          'p-2'\n        ]\"\n      />\n    </form> -->\n  </div>\n</template>\n\n<script>\n\nexport default {\n  name: 'PageButton',\n  components: {\n  },\n  data() {\n    return {\n      isActive: false,\n      timer: 0,\n      timerZero: 0,\n      interval: null,\n      status: null,\n      logs: null,\n      time: null,\n      counter: 0\n    }\n  },\n  computed: {\n    person() {\n      return this.$route.query.person\n    },\n    timestamp() {\n      if (this.isActive) {\n       var date = new Date(null);\n       date.setSeconds(this.timer);\n       return date.toISOString().substr(11, 8);\n      } else {\n        return '00:00:00';\n      }\n    }\n  },\n  methods: {\n    handleClick() {\n      this.isActive = !this.isActive;\n      this.status = JSON.parse(localStorage.getItem('status'));\n      this.logs = JSON.parse(localStorage.getItem('logs'));\n      this.time = JSON.parse(localStorage.getItem('time'));\n\n      this.status[this.person] = this.isActive;\n      this.time[this.person] = this.timer;\n      //\n      if (this.isActive) {\n        this.counter ++;\n        let _ = this\n        this.interval = setInterval(function () {\n          _.timer ++;\n          _.timerZero ++;\n          _.time = JSON.parse(localStorage.getItem('time'));\n          _.time[_.person] = _.timer;\n          localStorage.setItem('time', JSON.stringify(_.time))\n          const log = {person: _.person, duration: _.timerZero}\n          _.logs[`${_.counter + _.person}`] = log\n          localStorage.setItem('logs', JSON.stringify(_.logs))\n\n        }, 1000);\n      } else {\n        clearInterval(this.interval)\n        console.log(this.logs)\n        this.timerZero = 0;\n      }\n      localStorage.setItem('status', JSON.stringify(this.status))\n    }\n  },\n  mounted() {\n    this.status = JSON.parse(localStorage.getItem('status'));\n    this.logs = JSON.parse(localStorage.getItem('logs'));\n    this.time = JSON.parse(localStorage.getItem('time'));\n    this.isActive = this.status[this.person];\n  }\n}\n</script>\n\n\n<style lang=\"scss\">\n\n\n</style>\n"],"sourceRoot":""}]}