.1\" id=\"Ebene_1\" xmlns=\"http://www.w3.org/1200/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" x=\"0px\" y=\"0px\"\n        viewBox=\"0 0 12 12\" xml:space=\"preserve\" width=\"12\" height=\"12\"\n        :class=\"[\n          'transition-all duration-500',\n          /* selectedItem === index ? 'bg-red-500' : '' */\n          {\n            'animate-spin' : selectedItems.includes(index)\n          }\n        ]\"\n      >\n        <g>\n          <g>\n            <line class=\"st0\" x1=\"0\" y1=\"0\" x2=\"12px\" y2=\"12px\"/>\n            <line class=\"st0\" x1=\"12px\" y1=\"0\" x2=\"0px\" y2=\"12px\"/>\n            <rect class=\"st3\" x=\"0\" y=\"0\"  width=\"12px\" height=\"12px\"/>\n          </g>\n        </g>\n\n      </svg>\n      <!-- <div\n        :style=\"{maxWidth: '10rem'}\"\n        :class=\"[\n          'pointer-events-none',\n          'text-center',\n          'mt-2',\n          selectedItems.includes(index) ? 'opacity-1' : 'opacity-0'\n        ]\"\n      >\n\n        {{research[index-1].name}}\n      </div> -->\n    </div>\n  </div>\n\n</template>\n\n<script>\n\n  export default {\n    props: {\n      research: Array\n    },\n    data() {\n      return {\n        selectedItems: [],\n      }\n    },\n    computed: {\n      positions() {\n        let positionArray = []\n        for (let i=0; i < this.research.length; i++) {\n          positionArray.push([ Math.floor(Math.random() * 60) + 20,  Math.floor(Math.random() * 60) + 20])\n        }\n        return positionArray;\n      }\n    },\n    methods: {\n      // showItem(i) {\n      //   if (this.selectedItem === i) {\n      //     this.selectedItem = null\n      //   } else {\n      //     this.selectedItem = i;\n      //   }\n      //\n      //   this.$emit('showItem', this.selectedItem)\n      // },\n      handleClick(i) {\n        if (this.selectedItems.includes(i)) {\n          this.selectedItems = []\n          this.$emit('hoverItem', this.selectedItems)\n        }\n      },\n      handleHover(i) {\n        if (!this.selectedItems.includes(i)) {\n          this.selectedItems = [i]\n          this.$emit('hoverItem', this.selectedItems)\n        }\n      }\n    },\n    mounted() {\n  }\n}\n\n</script>\n\n<style lang=\"scss\" scoped>\n\n.st0{fill:none;stroke:#000000;stroke-miterlimit:10;}\n.st1{fill-rule:evenodd;clip-rule:evenodd;fill:none;}\n.st3{fill:rgba(0,0,0,0);stroke-miterlimit:10;}\n\ng {\n  cursor: pointer;\n}\nsvg {\n  top: 0;\n  left: 0;\n}\n\n</style>\n"],"sourceRoot":""}]}